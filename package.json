{
  "name": "Test Oston Moove - Back-end",
  "version": "1.0.0",
  "description": "Api of business registration",
  "main": "src/shared/infra/http/server.ts",
  "scripts": {
    "dev": "nodemon",
    "build": "rimraf ./build && tsc",
    "start": "npm run build && node build/src/server.js",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli ",
    "migration:create": "typeorm migration:create \" /src/shared/infra/typeorm/migrations",
    "migration:run": "ts-node --transpile-only ./node_modules/typeorm/cli.js migration:run -d data-source.ts",
    "migrate:revert": "ts-node --transpile-only ./node-modules/typeorm/cli.js migration:revert",
    "deploy": "git add . && git commit -m \" && git push origin main"
  },
  "author": "Wendel S. Pereira",
  "license": "ISC",
  "dependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^18.0.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "heroku": "^7.60.2",
    "mysql": "^2.18.1",
    "reflect-metadata": "^0.1.13",
    "tsconfig-paths": "^4.0.0",
    "typeorm": "^0.3.7"
  },
  "devDependencies": {
    "nodemon": "^2.0.19",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
